{{!
    @template theme_trend/footer

    Footer template

    Example context (json): {}
}}

  <footer id="page-footer" class="p-y-1">
      <div class="footer-container">

          <div id="footer-contact" class="footer-box">
                {{{ output.footer_contact }}}
          </div>

          <div  class="footer-box">
          <nav class="nav navbar-nav hidden-lg-up">
              {{# output.custom_menu_flat }}
                  <ul class="list-unstyled p-t-1">
                      {{> theme_trend/custom_menu_footer }}
                  </ul>
              {{/ output.custom_menu_flat }}
          </nav>
          </div>

          <div id="course-footer"  class="footer-box">{{{ output.course_footer }}}</div>

          {{{ output.standard_footer_html }}}
          
      </div>
    {{{ output.footer_copy }}}
  </footer>

{{{ output.standard_end_of_body_html }}}
